index 3dca83305..2759a1c5c 100644
--- a/vendor/libc/src/unix/mod.rs
+++ b/vendor/libc/src/unix/mod.rs
@@ -348,12 +348,7 @@ cfg_if! {
         #[link(name = "c", cfg(not(target_feature = "crt-static")))]
